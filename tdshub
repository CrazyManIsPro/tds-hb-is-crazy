local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/CrazyManIsPro/tds-hb-is-crazy/refs/heads/main/tdhubsource",true))()

local Window = OrionLib:MakeWindow({Name = "TDS HUB", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

--[[
Name = <string> - The name of the UI.
HidePremium = <bool> - Whether or not the user details shows Premium status or not.
SaveConfig = <bool> - Toggles the config saving in the UI.
ConfigFolder = <string> - The name of the folder where the configs are saved.
IntroEnabled = <bool> - Whether or not to show the intro animation.
IntroText = <string> - Text to show in the intro animation.
IntroIcon = <string> - URL to the image you want to use in the intro animation.
Icon = <string> - URL to the image you want displayed on the window.
CloseCallback = <function> - Function to execute when the window is closed.
]]

local TabMisc = Window:MakeTab({
    Name = "Misc",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

TabMisc:AddButton({
    Name = "Mobile fly",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt"))()
    end
})

local TabWarMachine = Window:MakeTab({
    Name = "War Machine",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

TabWarMachine:AddToggle({
    Name = "Auto Farm Money",
    Default = false,
    Callback = function(Value)
        if Value then
            loadstring(game:HttpGet("https://raw.githubusercontent.com/xtrey10x/xtrey10x-hub/main/War%20Machines"))()
        end
    end    
})

local TabPogoObby = Window:MakeTab({
    Name = "Pogo Obby",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

TabPogoObby:AddButton({
    Name = "Pro Hub",
    Callback = function()
        loadstring(game:HttpGet("https://rawscripts.net/raw/W10-Pogo-Obby-inf-22506"))()
    end
})

local TabTowerOfHell = Window:MakeTab({
    Name = "Tower Of Hell",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

TabTowerOfHell:AddButton({
    Name = "TOH HAX",
    Callback = function()
        loadstring(game:HttpGet("https://rawscripts.net/raw/Tower-of-Hell-Hax-5597"))()
    end
})

TabTowerOfHell:AddButton({
    Name = "Tower Of Hacks",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/scowscripts/scowsscripts/main/TowerOfHacks/Script", true))()
    end
})

local TabFisch = Window:MakeTab({
    Name = "Fisch",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

TabFisch:AddButton({
    Name = "Fischh",
    Callback = function()
        loadstring(game:HttpGet("https://pastebin.com/raw/q7j7nAf0",true))()
    end
})
